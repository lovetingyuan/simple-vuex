{
  "name": "@tingyuan/vue-store",
  "version": "0.0.15",
  "description": "simpler vuex alternative",
  "main": "dist/vue-store.common.js",
  "module": "dist/vue-store.esm.js",
  "unpkg": "dist/vue-store.min.js",
  "jsdelivr": "dist/vue-store.min.js",
  "types": "./types.ts",
  "sideEffects": false,
  "directories": {
    "example": "example",
    "lib": "lib"
  },
  "files": [
    "dist",
    "types"
  ],
  "scripts": {
    "build": "npm run lint && rm -rf dist && npx rollup -c ./rollup.config.js",
    "lint": "npx eslint --ext .js,.ts lib/vue-store.ts --fix",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lovetingyuan/simple-vuex.git"
  },
  "keywords": [
    "vuex",
    "vue",
    "store",
    "state",
    "management",
    "redux",
    "flux",
    "reducer"
  ],
  "author": "tingyuan",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lovetingyuan/vue-store/issues"
  },
  "homepage": "https://github.com/lovetingyuan/vue-store#readme",
  "devDependencies": {
    "@types/node": "^13.7.0",
    "tslib": "^1.11.0",
    "vue": "^2.6.11",
    "vuex": "^3.1.2",
    "@typescript-eslint/eslint-plugin": "^2.21.0",
    "eslint": "^6.8.0",
    "eslint-config-standard-with-typescript": "^14.0.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "rollup": "^1.31.0",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-terser": "^5.2.0",
    "rollup-plugin-typescript": "^1.0.1",
    "typescript": "^3.8.2"
  },
  "eslintConfig": {
    "extends": "standard-with-typescript",
    "parserOptions": {
      "project": "./tsconfig.json"
    },
    "rules": {
      "@typescript-eslint/strict-boolean-expressions": "off"
    }
  },
  "eslintIgnore": [
    "dist",
    "example",
    "rollup.config.js"
  ]
}
